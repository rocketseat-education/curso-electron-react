    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-react": "^2.1.0",
    "autoprefixer": "^10.4.13",
    "electron": "^21.2.2",
    "electron-builder": "^23.6.0",
    "electron-notarize": "^1.2.1",
    "electron-vite": "^1.0.11",
    "eslint": "^8.25.0",
    "postcss": "^8.4.18",
    "rollup-plugin-inject-process-env": "^1.3.1",
    "tailwind-scrollbar": "^2.0.1",
